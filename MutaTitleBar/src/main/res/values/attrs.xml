<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MutaTitlebar">
        <attr name="title" format="string" />
        <attr name="titleTextColor" format="color" />
        <attr name="icon" format="reference" />
        <attr name="cancelIcon" format="reference" />
        <attr name="startColor" format="color" />
        <attr name="endColor" format="color" />
    </declare-styleable>

    <declare-styleable name="MaterialLayout">
        <attr name="alpha_color" format="integer" />
        <attr name="alpha_step" format="integer" />
        <attr name="framerate" format="integer" />
        <attr name="duration" format="integer" />
        <attr name="color" format="color" />
        <attr name="scale" format="float" />
    </declare-styleable>

</resources>